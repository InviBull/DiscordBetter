{
    "manifest_version":2,
    "name":"DiscordBetter",
    "version":"1.0",
    "description":"A Web App Plugin",
    "content_scripts":[
        {
          "matches":["https://discord.com/*"],
          "js":["app.js"]
        }
    ],
      "permissions":[
        "https://discord.com/*"
    ]
}